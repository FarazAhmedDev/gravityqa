# AI Provider Configuration
# Choose one: openai, anthropic, or leave both for fallback
OPENAI_API_KEY=
ANTHROPIC_API_KEY=

# Default AI Settings
DEFAULT_LLM_PROVIDER=openai
DEFAULT_MODEL=gpt-4-vision-preview

# Database
DATABASE_URL=sqlite:///./gravityqa.db

# Appium Server
APPIUM_HOST=localhost
APPIUM_PORT=4723

# Playwright
PLAYWRIGHT_PORT=9323

# Application Settings
DEBUG=True
APP_NAME=GravityQA
APP_VERSION=1.0.0

# File Storage Paths
# Data will be stored in: ~/Library/Application Support/GravityQA/
# Leave empty to use default paths
DATA_DIR=
PROJECTS_DIR=
CHROMA_PERSIST_DIR=

# WebSocket
WS_MESSAGE_QUEUE_SIZE=100

# Security (Optional - for future use)
SECRET_KEY=
JWT_SECRET=
